{
  "exec-opts": ["native.cgroupdriver=systemd"],
  "registry-mirrors": {{ docker_registry_mirrors|unique|to_json }},
{% if docker_bridge_network is defined %}
  "bip": "{{ docker_bridge_network }}",
{% endif %}
{% if docker_bridge is defined %}
  "bridge": "{{ docker_bridge }}",
{% endif %}
  "icc": false,
  "log-level": "info",
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3",
    "compress": "true"
  }
}
