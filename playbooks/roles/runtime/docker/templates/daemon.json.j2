{
  "exec-opts": ["native.cgroupdriver=systemd"],
  "registry-mirrors": {{ docker_registry_mirrors|to_json }},
  "insecure-registries": {{ INSECURE_REGISTRIES|to_json }},
  "icc": false,
  "log-level": "info",
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "10m",
    "max-file": "3",
    "compress": "true"
  }
}
